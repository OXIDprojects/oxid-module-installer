{
  "name": "oxid-community/moduleinstaller",
  "description": "Install Composer Packages via GUI.",
  "type": "oxideshop-module",
  "minimum-stability": "stable",
  "keywords": [
    "oxid",
    "modules",
    "admin",
    "gui",
    "composer",
    "installer",
    "composer GUI"
  ],
  "homepage": "https://github.com/OXIDprojects/oxid-module-installer",
  "license": [
    "GPL-3.0"
  ],
  "require":{
    "composer/composer" : "^1.6",
    "oxid-esales/oxideshop-ce" : "^6.0"
  },
  "extra": {
    "oxideshop": {
      "target-directory": "oxcom/moduleinstaller"
    }
  },
  "autoload": {
    "psr-4": {
      "OxidCommunity\\ModuleInstaller\\": "./"
    }
  }
}
